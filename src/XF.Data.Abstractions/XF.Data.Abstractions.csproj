<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>XF.Data.Abstractions.dll</AssemblyName>
    <PackageVersion>1.0.0.0</PackageVersion>
    <PackageVersionPrefix>pre</PackageVersionPrefix>
    <PackageVersionSuffix>suf</PackageVersionSuffix>
    <Authors>eXtensoft Contributors</Authors>
    <Title>eXtensible Data</Title>
